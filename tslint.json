{
  "extends": "./node_modules/@chemzqm/tslint-config/tslint.json",
  "rules": {
    "no-unbound-method": false,
    "no-unused-expression": false,
    "return-undefined": false,
    "no-unused-variable": [true, {"ignore-pattern": "^(_|logger)"}],
    "no-console": [false, "log", "info", "warn", "error", "trace", "time", "timeEnd", "assert", "count", "dir", "table"]
  }
}
